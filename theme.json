{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"patterns": [
		"featured-section-with-round-corners"
	],
	"settings": {
		"layout": {
			"contentSize": "750px",
			"wideSize": "1200px"
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Open Sans",
					"name": "Open Sans",
					"slug": "open-sans",
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontWeight": "200",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/OpenSans/OpenSans-Light.woff2",
								"file:./assets/fonts/OpenSans/OpenSans-Light.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontWeight": "200",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/OpenSans/OpenSans-LightItalic.woff2",
								"file:./assets/fonts/OpenSans/OpenSans-LightItalic.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/OpenSans/OpenSans-Regular.woff2",
								"file:./assets/fonts/OpenSans/OpenSans-Regular.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/OpenSans/OpenSans-Italic.woff2",
								"file:./assets/fonts/OpenSans/OpenSans-Italic.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/OpenSans/OpenSans-Bold.woff2",
								"file:./assets/fonts/OpenSans/OpenSans-Bold.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontWeight": "700",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/OpenSans/OpenSans-BoldItalic.woff2",
								"file:./assets/fonts/OpenSans/OpenSans-BoldItalic.ttf"
							]
						}
					]
				}
			]
		}
	}
}
